{
  "name": "machadovilaca.github.io",
  "version": "0.0.2",
  "description": "João Vilaça personal website",
  "repository": "https://github.com/machadovilaca/machadovilaca.github.io",
  "homepage": "https://machadovilaca.github.io",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "build": "webpack --config webpack.prod.js",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "predeploy": "webpack --config webpack.prod.js && echo machadovilaca.eu > dist/CNAME && cp dist/index.html dist/404.html",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/leaflet": "^1.9.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/victory": "^33.1.5",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv-webpack": "^7.1.1",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.0.0",
    "html-webpack-plugin": "^5.5.3",
    "imagemin": "^8.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^1.6.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^5.3.4",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.6.0",
    "typescript": "^4.9.5",
    "typescript-plugin-css-modules": "^3.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@patternfly/react-catalog-view-extension": "^5.0.0",
    "@patternfly/react-core": "^5.1.0",
    "@patternfly/react-icons": "^5.1.0",
    "@patternfly/react-styles": "^5.1.0",
    "leaflet": "^1.9.4",
    "monaco-editor": "^0.21.3",
    "monaco-editor-webpack-plugin": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "sirv-cli": "^1.0.14"
  }
}
